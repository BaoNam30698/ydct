        {
            <div compile="vm.dirTraCuu"></div>
        }
        else if (ViewBag.Url.Contains("dongnai."))
        {
            <div ng-init="vm.tenantName = 'dongnai'"></div>
            <div compile="vm.dirTraCuu"></div>
        }
        else if (ViewBag.Url.Contains("hanoi."))
        {
            <div ng-init="vm.tenantName = 'hanoi'"></div>
            <div compile="vm.dirTraCuu"></div>
        }
        else if (ViewBag.Url.Contains("laocai."))
        {
            <div ng-init="vm.tenantName = 'laocai'"></div>
            <div compile="vm.dirTraCuu"></div>
        }
        else if (ViewBag.Url.Contains("vinhphuc."))

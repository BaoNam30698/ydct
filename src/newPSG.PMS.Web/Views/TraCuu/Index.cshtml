@using newPSG.PMS.Web.Navigation
@{
    ViewBag.CurrentPageName = PageNames.Frontend.Tracuu;
}

<style>
    .btn-blue {
        background: #4281c5;
        color: white;
    }
</style>

<div class="bread-crumb bg-blue-steel">
    <div class="bread-crumb-item">
        <div class="container">
            <ul class="page-breadcrumb breadcrumb">
                <li>
                    <a href="@Url.Action("Index", "Home")">Trang chủ</a>
                </li>
                <li>
                    <span class="active">Tra cứu</span>
                </li>
                <li>
                    <span class="active">@ViewBag.TenThuTuc</span>
                </li>
            </ul>
        </div>
    </div>
</div>


@using Abp.Authorization.Users
<div>
    <div id="selectProductModal" novalidate class="form-validation">
        <div class="modal-header">
            <h4 class="modal-title text-center text-uppercase">
               Hướng dẫn đăng ký công bố sản phẩm
                <button type="button" class="close" ng-click="vm.cancel()" aria-hidden="true"></button>
            </h4>
            
        </div>
        <div class="modal-body text-center">
            <iframe title="title" style="width:800px; height:600px;border:0"  src="https://www.youtube.com/embed/25k2UI1gVbw"   allow="autoplay; encrypted-media" allowfullscreen></iframe>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn red" ng-click="vm.cancel()"><em class="glyphicon glyphicon-remove"></em>@L("Cancel")</button>
        </div>
    </div>
</div>